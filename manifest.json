{
   "content_scripts": [ {
      "js": [ "TIM.global.js", "TIM.settings.js", "TIM.view.js", "TIM.convert.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "description": "How much time will that cost you?",
   "manifest_version": 2,
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "background": { "page": "ga.html" },
   "name": "Time is money",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "icons": { "16": "icon16.png",
              "48": "icon48.png",
             "128": "icon128.png" },
  "browser_action": {
    "default_title": "Time is Money",
    "default_icon":  "icon48.png",
    "default_popup": "options.html"
  },
  "options_ui":  { "page": "options.html", "chrome_style": false },
  "permissions": [ "storage" ],
  "version":     "1.60"
}
